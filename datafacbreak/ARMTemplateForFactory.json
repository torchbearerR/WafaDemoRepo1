					"transformations": [
						{
							"name": "aggregate1"
						},
						{
							"name": "join1"
						}
					],
					"scriptLines": [
						"source(output(",
						"          empid as string,",
						"          name as string,",
						"          { country} as string,",
						"          { department} as string",
						"     ),",
						"     allowSchemaDrift: true,",
